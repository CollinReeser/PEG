Expr    :: Sum ;
Product :: Value * ( ( # capt ( | '*' '/' ) "" ) Value ) $ foldStack ;
Sum     :: Product * ( ( # capt ( | '+' '-' ) "" ) Product ) $ foldStack ;
Value   :: | ( num "" ) ( "(" Expr ")" ) ;

num     :: # capt ( + ( | '1' || '2' || '3' || '4' || '5' || '6' || '7' || '8' || '9' '0' ) ) ;
